{
  "name": "arraypress/wp-block-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for working with blocks and Gutenberg content",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/block-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\BlockUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-block-utils/issues",
    "source": "https://github.com/arraypress/wp-block-utils"
  },
  "keywords": [
    "wordpress",
    "blocks",
    "gutenberg",
    "wp",
    "arraypress",
    "block-editor",
    "parse-blocks",
    "render-blocks",
    "block-utils"
  ]
}